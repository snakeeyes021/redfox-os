[ookla_speedtest-cli]
name=ookla_speedtest-cli
# VERIFIED: Using el/9 as it contains valid 2022 metadata (Fedora 37+ are empty)
baseurl=https://packagecloud.io/ookla/speedtest-cli/el/9/$basearch
enabled=1
gpgcheck=0
repo_gpgcheck=1
gpgkey=https://packagecloud.io/ookla/speedtest-cli/gpgkey
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
metadata_expire=300
# Priority override to ensure this 'speedtest' beats the Fedora GUI 'speedtest'
cost=10